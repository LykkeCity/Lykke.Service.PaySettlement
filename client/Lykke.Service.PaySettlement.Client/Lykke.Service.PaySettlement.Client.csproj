<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.1</Version>
    <Authors>Lykke</Authors>
    <Company>Lykke</Company>
    <Product>Lykke.Service.PaySettlement</Product>
    <Description>Client for Lykke.Service.PaySettlement API</Description>
    <PackageLicenseUrl>https://github.com/LykkeCity/Lykke.Service.PaySettlement/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/LykkeCity/Lykke.Service.PaySettlement</PackageProjectUrl>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/14153330?v=3&amp;s=200</PackageIconUrl>
    <RepositoryUrl>https://github.com/LykkeCity/Lykke.Service.PaySettlement.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Lykke</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Lykke.Service.PaySettlement.Client.xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\src\Lykke.Service.PaySettlement.Core\Domain\SettlementStatus.cs" Link="Models\SettlementStatus.cs" />
    <Compile Include="..\..\src\Lykke.Service.PaySettlement\Models\ContinuationResult.cs" Link="Models\ContinuationResult.cs" />
    <Compile Include="..\..\src\Lykke.Service.PaySettlement\Models\PaymentRequestModel.cs" Link="Models\PaymentRequestModel.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Lykke.HttpClientGenerator" Version="2.1.2" />
    <PackageReference Include="Lykke.Service.PayInternal.Contract" Version="2.0.4" />
    <PackageReference Include="Lykke.SettingsReader" Version="4.0.1" />
    <PackageReference Include="Refit" Version="4.6.16" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>
</Project>